<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat code coverage">
				<Configuration>
					<Format>cobertura</Format>
					<ExcludeByFile>**/Migrations/ApplicationDbContextModelSnapshot.cs</ExcludeByFile>
					<ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,MigrationAttribute</ExcludeByAttribute>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>